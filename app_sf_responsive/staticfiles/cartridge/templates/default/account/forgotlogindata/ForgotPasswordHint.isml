<iscontent type="text/html" charset="UTF-8" compact="true">
<isGetLoginPreferenceReader>

<p>
	<istext key="#'account.forgotdata.password_retrieval_info.'.UserLoginType.'.text'#"/>
	<isif condition="#(isDefined(SecurityQuestionEnabled)) AND (SecurityQuestionEnabled EQ 'true')#">
		<istext key="account.forgotdata.password_retrieval_info.security_question.text"/>
	</isif>
	<istext key="account.forgotdata.password_retrieval_info.follow_link.text" parameter0="#encodeValue(URL(Action('ViewContact-Start')))#" encoding="off"/>
</p>